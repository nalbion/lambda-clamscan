{
    "extends": "tslint-eslint-rules",
    "rules": {
        "no-unused-expression": true,
        "no-unused-new": true,
        "no-unused-variable": false,
        "member-access": false,
        "max-line-length": [
            true,
            160
        ],
        "no-duplicate-variable": true,
        "semicolon": [
            true,
            "always"
        ],
        "variable-name": [
            true,
            "ban-keywords"
        ]
    }
}
